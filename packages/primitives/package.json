{
  "name": "@cryptoeconomicslab/primitives",
  "version": "0.0.9",
  "description": "primitive types",
  "author": {
    "name": "Cryptoeconomics Lab",
    "url": "https://www.cryptoeconomicslab.com/"
  },
  "engines": {
    "node": ">=10"
  },
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc --build",
    "build:check": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "lint": "eslint --ext ts,tsx src",
    "doc": "typedoc --out docs src",
    "clean": "rm -rf lib docs",
    "prepublish": "npm run build"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/?(*.)+(spec|test).(ts|js)",
      "**/?(*.)+(spec|test).ts"
    ],
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    }
  },
  "dependencies": {
    "text-encoding": "^0.7.0"
  },
  "devDependencies": {
    "@types/text-encoding": "0.0.35"
  },
  "gitHead": "4fa17adb78eb327c05d5af035a391566738f427b"
}
